@page "/departmentpage"

<h3>Daftar Department</h3>

<table>
    <thead>
        <tr>
            <th>Department ID</th>
            <th>Department Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var dept in department)
        {
            <tr>
                <td>@dept.DepartmentID</td>
                <td>@dept.DepartmentName</td>
            </tr>
        }
    </tbody>
</table>